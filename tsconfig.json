{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "strict": true,
        "skipLibCheck": true,
        "lib": ["ESNext"],
        "jsx": "react-jsx",
        "jsxImportSource": "hono/jsx",
        "plugins": [
            {
                "name": "typescript-lit-html-plugin",
                "tags": ["html"]
            }
        ]
    }
}
